{
    "keyboard_name": "Keychron K17 Max",
    "manufacturer": "Keychron",
    "url": "https://github.com/Keychron",
    "maintainer": "Keychron",
    "processor": "STM32F401",
    "bootloader": "stm32-dfu",
    "usb": {
        "vid": "0x3434"
    },
    "features": {
        "bootmagic": true,
        "extrakey": true,
        "mousekey": true,
        "dip_switch": true,
        "encoder": true,
        "encoder_map": true,
        "nkro": true,
        "raw": true,
        "send_string": true
    },
    "diode_direction": "ROW2COL",
    "dip_switch": {
        "pins": ["B14"]
    },
    "eeprom": {
        "wear_leveling": {
            "driver": "embedded_flash",
            "logical_size": 2048,
            "backing_size": 4096
        }
    },
    "encoder": {
        "rotary": [
            {
                "pin_a": "A8",
                "pin_b": "C9"
            }
        ]
    },
    "dynamic_keymap": {
        "layer_count": 4
    }
}
